/*!
 * tanguage framework source code
 *
 * static highlight.language
 *
 * Date: 2017-04-06
 */
;
use ~/../;
..highlight.languages.brainfuck = {
    'pointer': {
        pattern: /<|>/,
        alias: 'keyword'
    },
    'increment': {
        pattern: /\+/,
        alias: 'inserted'
    },
    'decrement': {
        pattern: /-/,
        alias: 'deleted'
    },
    'branching': {
        pattern: /\[|\]/,
        alias: 'important'
    },
    'operator': /[.,]/,
    'comment': /\S+/
};
